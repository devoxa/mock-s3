{
  "name": "@devoxa/mock-s3",
  "description": "Mock S3 server for test environments, based on s3rver",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/mock-s3"
  },
  "scripts": {
    "start": "ts-node-dev --exit-child --respawn --rs src/index.ts",
    "format": "prettier --ignore-path='.gitignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.gitignore' --check .",
    "lint": "eslint 'src/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "prettier": "@devoxa/prettier-config",
  "dependencies": {
    "s3rver": "3.7.1"
  },
  "devDependencies": {
    "@devoxa/eslint-config": "4.0.2",
    "@devoxa/prettier-config": "2.0.3",
    "@types/node": "20.9.5",
    "@types/s3rver": "3.7.4",
    "eslint": "9.30.1",
    "prettier": "3.6.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  },
  "volta": {
    "node": "20.9.0"
  }
}
